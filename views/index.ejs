<% include partials/header %>

<div class="ui main text container segment">
  <div class="ui huge header">Blogs</div>

    <% blogs.forEach(function(blog){ %>
        <div>
          <h2><%= blog.title %></h2>
          <img src="<%= blog.image %>" width="400px">
          <span><%= blog.created %></span>
          <p><%= blog.body %></p>
        </div>
    <% }); %>
    
</div>

<% include partials/footer %>
